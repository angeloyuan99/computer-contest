#include<iostream>
#include<cmath>
using namespace std;
bool isPrime(int n){
    if (n <= 1)
        return false;
    if (n <= 3)
        return true;
    if (n % 2 == 0 || n % 3 == 0)
        return false;
  
    for (int i = 5; i * i <= n; i = i + 6)
        if (n % i == 0 || n % (i + 2) == 0)
            return false;
    return true;
}
int countFactors(int n, int p){
    int result = 0;
    for(; n >= p; n /= p){
        result += n / p;
    }
    return result;
}
int main(){
    for(int j = 0; j < 5; j++){
        int n;
        cin >> n;
        bool head = true;
        for(int i = 2; i <= n; i++){
            if(!isPrime(i)){
                continue;
            }
            if(head){
                head = false;
            } else{
                cout << " * ";
            }
            cout << i << "^" << countFactors(n, i);
        }
        cout << "\n";
    }
}